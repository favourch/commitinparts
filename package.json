{
  "name": "commitinparts",
  "displayName": "Commit In Parts",
  "description": "The CommitInParts extension for Visual Studio Code allows you to break the contents of a file into parts and commit each part separately. This extension enhances your workflow by providing fine-grained control over commits, allowing you to commit changes in smaller, logical portions.",
  "publisher": "Favour Chukwuedo",
  "repository": {
    "type": "git",
    "url": "https://github.com/favourch/commitinparts"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "extension.commitInParts",
      "title": "Commit File Parts"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
